{
	"compilerOptions": {
	  "target": "ES2022",
	  "module": "Node16",
	  "outDir": "./out",
	  "sourceMap": true,
	  "esModuleInterop": true,
	  "strict": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "lib": [
			"ES2022"
		],
	  "baseUrl": ".",
	  "paths": {
		"vscode": ["./node_modules/@types/vscode"]
	  }
	},
	"include": [
	  "src/**/*"
	],
	"exclude": [
	  "node_modules",
	  ".vscode-test"
	]
  }